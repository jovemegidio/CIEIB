<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#c8a951</color>
    <color name="colorBackground">#1a3a5c</color>
    <color name="colorPrimary">#1a3a5c</color>
    <color name="colorPrimaryDark">#0f2440</color>
    <color name="navigationColor">#0f2440</color>
    <color name="splashBackground">#1a3a5c</color>
    <color name="white">#ffffff</color>
    <string name="app_name">CIEIB</string>
    <string name="fallbackType">customtabs</string>
    <style name="Theme.CIEIB" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/colorBackground</item>
        <item name="android:navigationBarColor">@color/navigationColor</item>
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
    </style>
    <style name="Theme.CIEIB.Splash" parent="Theme.CIEIB">
        <item name="android:windowBackground">@color/splashBackground</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
    </style>
</resources>